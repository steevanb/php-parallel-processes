{
    "name": "steevanb/php-parallel-processes",
    "description": "Call processes in parallel",
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Steevan BARBOYON",
            "email": "steevan.barboyon@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "steevanb/php-typed-array": "^3.2",
        "symfony/console": "^5.2",
        "symfony/process": "^5.0"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "phpunit/phpunit": "9.5.*",
        "symfony/var-dumper": "5.3.*"
    },
    "autoload": {
        "psr-4": {
            "Steevanb\\ParallelProcess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "steevanb\\PhpTypedArray\\Bridge\\Phpstan\\": "vendor/steevanb/php-typed-array/bridge/Phpstan/src",
            "Steevanb\\ParallelProcess\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "@rm-composer-lock"
        ],
        "post-update-cmd": [
            "@rm-composer-lock"
        ],
        "rm-composer-lock": "rm composer.lock"
    }
}
