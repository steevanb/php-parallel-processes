{
    "name": "steevanb/php-parallel-processes",
    "description": "Call processes in parallel",
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Steevan BARBOYON",
            "email": "steevan.barboyon@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-pcntl": "*",
        "steevanb/php-collection": "^5.0",
        "symfony/console": "^6.1",
        "symfony/process": "^6.1"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "phpunit/phpunit": "9.5.*",
        "symfony/var-dumper": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "Steevanb\\ParallelProcess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Steevanb\\ParallelProcess\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "@rm-composer-lock"
        ],
        "post-update-cmd": [
            "@rm-composer-lock"
        ],
        "rm-composer-lock": "rm composer.lock"
    }
}
